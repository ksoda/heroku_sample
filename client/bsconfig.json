  "refmt": 3,
  "bs-dependencies": ["reason-react", "reason-apollo", "reason-apollo-hooks"],
  "bs-dev-dependencies": ["@glennsl/bs-jest"],
  "ppx-flags": ["@baransu/graphql_ppx_re/ppx", "get_in_ppx/ppx"],
