{
  "name": "my-react-app",
  "version": "0.1.0",
  "scripts": {
    "build": "npm run build:bs && npm run build:js && npm run build:cp",
    "build:bs": "bsb -make-world",
    "build:js": "webpack --mode=production --entry ./src/Main.bs.js --output dist/index.js",
    "build:cp": "cp indexProduction.html dist/index.html && cp dist/* ../server/static/",
    "start": "bsb -make-world -w -ws _ ",
    "clean": "bsb -clean-world",
    "server": "moduleserve ./ --port 8000",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "reason-react": ">=0.7.0"
  },
  "devDependencies": {
    "bs-platform": "^7.2.2",
    "moduleserve": "^0.9.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
