{
  "name": "my-todo-app",
  "version": "0.1.0",
  "scripts": {
    "build": "npm run build:bs && npm run build:js && npm run build:cp",
    "build:bs": "bsb -make-world",
    "build:js": "webpack --mode=production --entry ./src/Main.bs.js --output dist/index.js",
    "build:cp": "cp indexProduction.html dist/index.html && cp dist/* ../server/static/",
    "start": "npm run serve:db | npm run serve | bsb -make-world -w -ws _ ",
    "clean": "bsb -clean-world",
    "serve": "moduleserve ./ --port 8000",
    "serve:db": "node bin/genstub.js | tee /tmp/db.json && json-server --watch /tmp/db.json",
    "test": "npm run build:bs && jest"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@glennsl/bs-json": "^5.0.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "reason-react": ">=0.7.0"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.5.1",
    "bs-platform": "^7.2.2",
    "json-schema-faker": "^0.5.0-rcv.24",
    "json-server": "^0.16.1",
    "moduleserve": "^0.9.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
