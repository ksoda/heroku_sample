{
  "name": "my-todo-app",
  "version": "0.1.0",
  "scripts": {
    "dev": "concurrently \"bsb -clean-world -make-world -w\" \"next dev\"",
    "dev:reason": "bsb -clean-world -make-world -w",
    "dev:next": "next dev",
    "build": "npm run build:reason && next build",
    "build:reason": "bsb -clean-world -make-world",
    "start": "next start",
    "clean": "bsb -clean-world",
    "format": "find bindings/ components/ pages/ src/ __tests__/ -name '*.re' | xargs refmt --in-place",
    "test": "npm run build:reason && jest"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@glennsl/bs-json": "^5.0.2",
    "bs-fetch": "^0.5.2",
    "next": "^9.5.2",
    "next-transpile-modules": "^4.1.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "reason-react": ">=0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@glennsl/bs-jest": "^0.5.1",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/webpack": "^4.41.13",
    "babel-jest": "^26.3.0",
    "bs-platform": "^7.2.2",
    "concurrently": "^5.3.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.6",
    "react-test-renderer": "^16.13.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.re": "refmt --in-place"
  }
}
