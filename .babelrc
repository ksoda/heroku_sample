{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": "last 1 Chrome version, last 1 iOS version, last 1 ChromeAndroid version",
        "uglify": true
      },
      "useBuiltIns": true
    }]
  ],

  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    ["transform-class-properties", { "spec": true }]
  ]
}
